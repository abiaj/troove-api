{
  "name": "troove",
  "version": "1.0.0",
  "description": "Troove API built with NodeJS, Express, MongoDB and TypeScript",
  "main": "dist/server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "dev": "ts-node ./src/server.ts",
    "migration-gen": "typeorm migration:generate -n UserMigration",
    "migration-run": "typeorm migration:run",
    "migration-revert": "typeorm migration:revert",
    "start": "sudo nodemon ./dist/server.js",
    "prod": "npm run build && npm run start"
  },
  "keywords": [
    "user",
    "location"
  ],
  "author": "Prem Baboo <premb.2012@gmail.com> (http://huulke.com)",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.16.1",
    "@types/node": "^11.11.3",
    "bcrypt": "^3.0.5",
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.4",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "mongoose": "^5.4.17",
    "mysql": "^2.14.1",
    "nodemon": "^1.18.10",
    "reflect-metadata": "^0.1.10",
    "typeorm": "^0.3.0-alpha.23"
  }
}
